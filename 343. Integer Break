/**
  * https://leetcode.com/problems/integer-break/
  */

public class Solution {
    public int integerBreak(int n) {
        if(n<4) return n-1;
        if(n==4) return n;
        int ret = 1;
        while(n!=0 && n!=2 && n!=4){
            ret *= 3;
            n -= 3;
        }
        return n==0? ret : ret*n;
    }
}
